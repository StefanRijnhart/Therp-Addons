<?xml version="1.0"?>
<openerp>
    <data>
        <record id="hr_timesheet_line_search" model="ir.ui.view">
            <field name="name">hr.analytic.timesheet.search</field>
            <field name="model">hr.analytic.timesheet</field>
            <field name="type">search</field>
            <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_search" />
            <field name="groups_id" eval="[(6, 0, [ref('group_external_user_hr_timesheet')])]" />
            <field name="priority">999</field>
            <field name="arch" type="xml">
                <data>
                    <field name="account_id" position="replace" />
                    <group string="Group By..." position="replace" />
                    <field name="sheet_id" position="replace" />
                </data>
            </field>
        </record>
        <record id="hr_timesheet_line_tree" model="ir.ui.view">
            <field name="name">hr.analytic.timesheet.tree</field>
            <field name="model">hr.analytic.timesheet</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_tree" />
            <field name="groups_id" eval="[(6, 0, [ref('group_external_user_hr_timesheet')])]" />
            <field name="priority">999</field>
            <field name="arch" type="xml">
                <data>
                    <field name="journal_id" position="replace" />
                    <field name="account_id" position="replace" />
                    <field name="product_id" position="replace" />
                    <field name="product_uom_id" position="replace"/>
                    <field name="amount" position="replace" />
                    <field name="general_account_id" position="replace" />
                </data>
            </field>
        </record>
    </data>
</openerp>
