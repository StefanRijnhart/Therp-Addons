<?xml version="1.0"?>
<openerp>
    <data noupdate="1">
        <record model="ir.rule" id="ir_filter_user_rule">
            <field name="name">A user can only manage his or her own filters, and cannot assign another user</field>
            <field name="model_id" ref="base.model_ir_filters"/>
            <field name="domain_force">[
            ('create_uid', '=', user.id),
            '|', ('user_id', '=', False),
                 ('user_id', '=', user.id)
            ]</field>
            <field name="perm_read" eval="False"/>
        </record>
    </data>
</openerp>
